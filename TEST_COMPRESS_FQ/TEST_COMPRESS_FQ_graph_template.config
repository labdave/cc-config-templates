[utils_consolidatesamplename__tna]
    module      = Utils
    submodule   = ConsolidateSampleName

[utils_concatfastq__tna]
    module      = Utils
    submodule   = ConcatFastq

    final_output = R1, R2

[utils_springunzip__tna]
    module          = Utils
    submodule       = SpringUnzip
    input_from      = utils_concatfastq__tna
    docker_image    = spring

    final_output    = R1, R2

[utils_subsetfastq__tna]
    module          = Utils
    submodule       = SubsetFASTQ
    docker_image    = seqtk
    input_from      = utils_springunzip__tna

    final_output    = R1, R2

    [[args]]
        sample = 10000