[Docker]
    [[delly]]
        image = davelabhub/delly:latest
        [[[delly]]]
            resource_type   = delly
            path            = delly

[Path]
    [[ref]]
        resource_type   = ref
        path            = gs://davelab_data/ref/human/hg38/gencode/v30/GRCh38.p12.genome.plus.ERCC.fa*
    [[exclude_list]]
        resource_type   = exclude_list
        path            = gs://davelab_data/ref/human/hg38/human.hg38.excl.tsv