[Docker]
    [[gatk4]]
        image = broadinstitute/gatk:4.1.3.0
        [[[gatk4]]]
            resource_type   = gatk
            path            = gatk.jar
        [[[java]]]
            resource_type   = java
            path            = java

[Path]

    [[ref]]
        resource_type   = ref
        path            = gs://davelab_data/ref/human/hg38/gencode/v29/GRCh38.primary_assembly.genome.fa*

    [[ref_idx]]
        resource_type   = ref_idx
        path            = gs://davelab_data/ref/human/hg38/gencode/v29/GRCh38.primary_assembly.genome.fa.fai

    [[ref_dict]]
        resource_type   = ref_dict
        path            = gs://davelab_data/ref/human/hg38/gencode/v29/gatk4/GRCh38.primary_assembly.genome.dict

    [[target_bed]]
        resource_type   = bed
        path            = gs://davelab_data/ref/human/hg38/capture_baits/twist/1MB_10X_panel.bed
